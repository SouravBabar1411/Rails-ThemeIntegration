<div class="card">
  <div class="card-header">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Name:</strong>
      <%= @business.name %>
    </p>

    <p>
      <strong>Address:</strong>
      <%= @business.address %>
    </p>

    <p>
      <strong>start_date:</strong>
      <%= @business.start_date.strftime("at %I:%M %p %Y %m %d") %>
    </p>

    <p>
      <strong>end_date:</strong>
      <%= @business.end_date.strftime("at %I:%M %p %Y %m %d") %>
    </p>

      <%= link_to 'Edit', edit_business_path, class:"btn btn-secondary" %> <br><br>
      <%= link_to 'Back', businesses_path, class:"btn btn-secondary"%>
   </div>
</div>